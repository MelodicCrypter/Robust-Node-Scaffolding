{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:security/recommended",
        "eslint-config-airbnb-improved"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "security"
    ],
    "rules": {
        "no-console": 0,
        "indent": ["error", 4],
        "sort-imports": "off",
        "sort-keys": "off",
        "no-unused-vars": "off"
    }
}